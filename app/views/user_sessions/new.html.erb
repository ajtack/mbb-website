<h1>Log In</h1>

<% form_for @user_session, :url => user_session_path do |f| -%>
  <%= f.error_messages %>

  <p>
    <%= f.label :email, 'E-Mail' %><br />
    <%= f.text_field :email %>
  </p>
  
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  
  <p><%= submit_tag 'Log In' -%></p>
<% end -%>
