unless not @position_changed
	page.remove "member_#{@member.id}"
	unless @member.higher_item.nil?
		page.insert_html :after, "member_#{@member.higher_item.id}", :partial => 'member', :locals => {:member => @member}
	else
		unless @member.lower_item.nil?
			page.insert_html :before, "member_#{@member.lower_item.id}", :partial => 'member', :locals => {:member => @member}
		end
	end
end

